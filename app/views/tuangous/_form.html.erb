<%= simple_form_for(@tuangou, html: { multipart: true, class: "form-horizontal" }) do |f| %>
    <%= f.input :dealid, label: "团购ID" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :imgurl, label: "图片地址" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :originprice, label: "原价格" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :price, label: "现在价格" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :solds, label: "已售数量" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :stid, label: "stid" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :title, label: "标题" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
  <div class="col-sm-10 col-sm-offset-2">
    <%= f.button :submit, "提 交", class: "btn btn-primary" %>
    <%= link_to "返 回", tuangous_path, class: "btn btn-default" %>
  </div>
<% end %>
