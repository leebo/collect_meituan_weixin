<%= simple_form_for(@merchant, html: { multipart: true, class: "form-horizontal" }) do |f| %>
    <%= f.input :areaname, label: "地区名" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :avgprice, label: "平均价格" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :avgscore, label: "平均评分" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :catename, label: "分类名" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :channel, label: "channel" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :ctpoi, label: "ctpoi" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :frontimg, label: "frontimg" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :lat, label: "lat" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :lng, label: "lng" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :name, label: "店铺名称" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :poiid, label: "poiid" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :address, label: "地址" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
    <%= f.input :phone, label: "电话" %>
  <div class="line line-dashed b-b line-lg pull-in"></div>
  <div class="col-sm-10 col-sm-offset-2">
    <%= f.button :submit, "提 交", class: "btn btn-primary" %>
    <%= link_to "返 回", merchants_path, class: "btn btn-default" %>
  </div>
<% end %>
